{
    "name": "TwilioDevEd/call-forwarding-php",
    "description": "Use Twilio to forward calls.",
    "keywords": ["twilio", "call", "forward", "php"],
    "require": {
        "php": ">=5.6.4",
        "twilio/sdk": "^5.7",
        "klein/klein": "^2.1",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.7",
        "robmorgan/phinx": "^0.8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
